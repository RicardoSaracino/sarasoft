<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CutomersAddresses" table="cutomers_addresses">
    <indexes>
      <index name="created_by" columns="created_by"/>
      <index name="updated_by" columns="updated_by"/>
      <index name="address_id" columns="address_id"/>
      <index name="customer_id" columns="customer_id"/>
    </indexes>
    <id name="customer" association-key="true"/>
    <id name="address" association-key="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
    <one-to-one field="customer" target-entity="Customer" fetch="LAZY">
      <join-columns>
        <join-column name="customer_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="address" target-entity="Address" fetch="LAZY">
      <join-columns>
        <join-column name="address_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="createdBy" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="updatedBy" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="updated_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
