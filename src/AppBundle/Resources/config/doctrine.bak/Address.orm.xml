<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Address" table="address">
    <indexes>
      <index name="created_by" columns="created_by"/>
      <index name="updated_by" columns="updated_by"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="line1" type="string" column="line_1" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line2" type="string" column="line_2" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line3" type="string" column="line_3" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zipOrPostalcode" type="string" column="zip_or_postalcode" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="stateOrProvince" type="string" column="state_or_province" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="6" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
    <many-to-one field="createdBy" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="created_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="updatedBy" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="updated_by" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
